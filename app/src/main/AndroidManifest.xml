<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wmz.demo_w1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EventBusActivity"
            android:label="@string/eventBus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="LIST" />
            </intent-filter>
        </activity>
        <activity android:name=".EventBus2Activity" />
        <activity
            android:name=".SweetDialogActivity"
            android:label="@string/sweetDialog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="LIST" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NetworkActivity"
            android:label="@string/network">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="LIST" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdapterActivity"
            android:label="@string/adapter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="LIST" />
            </intent-filter>
        </activity>
        <activity android:name=".FragementActivity"
            android:label="@string/fragment">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="LIST"/>
            </intent-filter>
        </activity>

        <activity android:name=".BaseActivity"/>
    </application>

</manifest>